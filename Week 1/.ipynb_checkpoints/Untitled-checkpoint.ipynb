   "source": [
    "def roundoff (x):\n",
    "    remainder = x%1\n",
    "    if remainder <0.5:\n",
    "        print (\"Rounding down\")\n",
    "        x = x-1\n",
    "    else:\n",
    "        print (\"Rounding up\")\n",
    "        x = x+(1-remainder)\n",
    "        return (x)\n"
   ]
